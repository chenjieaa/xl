-button size=\"large\" @click=\"getMore\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n    import {Toast} from \"mint-ui\"\r\n    export default {\r\n        data(){\r\n            return {\r\n                msg:\"\",\r\n                pageIndex:0,\r\n                pageSize:5,\r\n                list:[]\r\n            }\r\n        },\r\n        created() {\r\n            this.getMore();\r\n        },\r\n        methods:{\r\n            getMore(){\r\n                this.pageIndex++;\r\n                var p = this.pageIndex;\r\n                var s = this.pageSize;\r\n                var id = this.id;\r\n                var url = \"getComent?id=\"+id+\"&pno=\"+p+\"&pageSize=\"+s;\r\n                this.$http.get(url).then(result=>{\r\n                    var row = this.list.concat(result.body.data)\r\n                    this.list = row;\r\n                })\r\n            },\r\n            postComment(){\r\n            var nid = this.id;\r\n            var pmsg = this.msg;\r\n            if(pmsg.trim().length==0){\r\n            Toast(\"评论内容不能为空\")\r\n            return;\r\n            }\r\n            var url = \"http://127.0.0.1:3000/postcomment\";\r\n            this.$http.post(url,{nid:nid,msg:pmsg}).then(result=>{\r\n                Toast(\"发表成功\")\r\n                this.pageIndex = 0;\r\n                this.list = [];\r\n                var t = document.querySelector(\".textarea\");\r\n                console.log(t);\r\n                this.t.innerHTML = \"\";\r\n                this.getMore();\r\n                \r\n            })\r\n            }\r\n        },\r\n        props:[\"id\"]\r\n    }\r\n</script>\r\n<style>\r\n    .app-comment textarea{\r\n        font-size:14px;\r\n        height:85px;\r\n        margin:0;\r\n    }\r\n    .app-comment .cmt-list{\r\n        margin:5px 0;\r\n    }\r\n    .app-comment .cmt-list .cmt-info{\r\n        line-height:30px;\r\n        background:#ccc;\r\n    }\r\n    .app-comment .cmt-list .cmt-bo