 if(this.val>1){this.val--;}\r\n            },\r\n            getImageList(){\r\n                this.$http.get(\"shoplist\").then(result=>{\r\n                    this.imglist = result.body;\r\n                })\r\n            }\r\n        },\r\n        created() {\r\n            this.getImageList();\r\n            this.getGoodInfo();\r\n        },\r\n        components:{\r\n            \"swiper-box\":swiper\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n.app-goodinfo .title,.price,.shuliang{\r\n    width:100%;\r\n    height:30px;\r\n    background:#fff;\r\n    font-size:16px;\r\n    color:#000;\r\n    line-height:30px;\r\n    padding-left:20px;\r\n}\r\n.app-goodinfo .price .prices{\r\n    color:red;\r\n}\r\n.app-goodinfo .button{\r\n    width:100%;\r\n    text-align:right;\r\n    margin-top:20px;\r\n}\r\n.app-goodinfo .xq img{\r\n    width:100%;\r\n    height:200px;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}